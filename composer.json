{
    "name": "thepinecode/translatable",
    "description": "Simple Eloquent model translations.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Gergo D. Nagy",
            "email": "dngege@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pine\\Translatable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pine\\Translatable\\Tests\\": "tests/"
        }
    },
    "require": {
        "php" : "^7.0"
    },
    "require-dev": {
        "orchestra/database": "^3.5",
        "orchestra/testbench": "^3.5"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pine\\Translatable\\TranslatableServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable" : true
}
